{
  "name": "BSRouter",
  "version": "0.1.0",
  "main": "dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "tsc && electron .",
    "build": "tsc && ng build && cp -rf conf dist/ && go build -o dist/bsrouter github.com/sutils/bsck/bsrouter",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "testSpider": "tsc && electron dist/spider/spider.saiguo.test.js",
    "pack-win": "npm run build && electron-packager ./ bsrouter --platform=win32 --arch=ia32 --out ./out --overwrite --ignore=\"^\\/(pkg.sh|e2e|spider|coverage|src|.vscode|angular.json|package-lock.json|tsconfig.json|tslint.json|main.ts|.editorconfig)\" --ignore=\"(.idea|docs|.gitignore|LICENSE|README.md|.DS_Store|bower.json)\" --download.mirror=https://npm.taobao.org/mirrors/electron/",
    "pack-osx": "npm run build && electron-packager ./ BSRouter --platform=darwin --out ./out --overwrite --ignore=\"^\\/(pkg.sh|e2e|spider|coverage|src|.vscode|angular.json|package-lock.json|tsconfig.json|tslint.json|main.ts|.editorconfig)\" --ignore=\"(.idea|docs|.gitignore|LICENSE|README.md|.DS_Store|bower.json)\" --download.mirror=https://npm.taobao.org/mirrors/electron/"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "lib/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ng-select/ng-select": "^2.4.0",
    "@types/bootstrap": "^4.1.2",
    "@types/jquery": "^3.3.5",
    "@types/log4js": "^2.3.5",
    "angular2-draggable": "^2.1.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "jquery": "^3.3.1",
    "log4js": "^3.0.5",
    "rxjs": "^6.0.0",
    "source-map-support": "^0.5.9",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "electron": "^2.0.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nyc": "^12.0.2",
    "protractor": "~5.3.0",
    "ts-node": "^5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "files": []
}